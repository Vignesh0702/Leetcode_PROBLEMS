//O(NLOGN)--- Sort the strings and equate , we will be able to figure out the anagram
class Solution {
public:
    bool isAnagram(string s, string t) {
       sort(s.begin(),s.end());
       sort(t.begin(),t.end());
       if(s==t)
       {
           return true;
       }
       else{
           return false;
       }
    }
};
